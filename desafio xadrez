#include <stdio.h>
int main() {

    printf("dasafio xadrez\n");
    printf("conmit\n");
    printf("\n");

    int i = 1;
    
    while (i <= 5){
        if(i % 1 <= 0)
        {
          printf("torre moveu %d casas para frente \n",i);                  
        }

        i++;
    }
     printf("\n");

    int b = 1;
 
    do {
        printf("bispo moveu %d casa para frente direita \n", b);
        b++;

    } while (b <= 5);

    printf("\n");

    for (int c = 1; c <= 8; c++)
    {
      printf("rainha moveu %d casas para esquerda \n", c);
    }

   
   return 0; 
}